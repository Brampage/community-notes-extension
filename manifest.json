{
  "manifest_version": 3,
  "name": "Community Notes",
  "description": "Shows Community Notes on web pages!",
  "version": "1.0",
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": "images/icon.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["<all_urls>"],
      "type": "module"
    }
  ]
}
